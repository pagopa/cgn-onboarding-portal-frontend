{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/main.{js,cjs,mjs,jsx,ts,cts,mts,tsx}"
  ],
  "project": ["**/*.{js,cjs,mjs,jsx,ts,cts,mts,tsx}"],
  "ignore": [
    "src/api/generated/**",
    "src/api/generated_backoffice/**",
    "src/api/generated_public/**",
    "src/types/**",
    "extensions/**",
    ".yarn/**"
  ]
}