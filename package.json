{
  "name": "cgn-onboarding-portal-frontend",
  "version": "3.33.6",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start:uat": "cp .env.uat-local .env.local && vite",
    "start:prod": "cp .env.prod-local .env.local && vite",
    "preview": "vite preview",
    "prebuild": "npm-run-all prebuild:*",
    "build": "yarn prebuild && tsc -b && vite build",
    "prebuild:generate": "yarn run generate",
    "prebuild:clean": "rm -rf dist",
    "prebuild:tsc": "yarn tsc:noEmit",
    "build:prod": "cp .env.prod .env.local && yarn build",
    "build:uat": "cp .env.uat .env.local && yarn build",
    "build:prod-local": "cp .env.prod-local .env.local && yarn build",
    "build:uat-local": "cp .env.uat-local .env.local && yarn build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint-autofix": "eslint . --fix",
    "generate": "./generate-openapi.sh",
    "prettify": "prettier --write \"./**/*.{ts,tsx}\"",
    "prepare": "husky",
    "postinstall": "patch-package",
    "tsc:noEmit": "tsc -b --noEmit",
    "analyzie-bundle-size": "npx vite-bundle-visualizer -t sunburst"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.39.0",
    "@hookform/lenses": "^0.7.1",
    "@hookform/resolvers": "^5.1.1",
    "@reduxjs/toolkit": "^2.11.2",
    "@tanstack/react-query": "^5.80.6",
    "@tanstack/react-table": "^8.21.3",
    "@types/lodash": "^4.17.17",
    "axios": "^1.8.4",
    "bootstrap-italia": "^2.14.0",
    "date-fns": "^4.1.0",
    "design-react-kit": "^5.8.2",
    "file-saver": "^2.0.5",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-datepicker": "^8.3.0",
    "react-dom": "^19.0.0",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.60.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^9.2.3",
    "zod": "^3.25.72"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/file-saver": "^2.0.2",
    "@types/node": "^24.0.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.0.4",
    "@types/react-google-recaptcha": "^2.1.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-table": "^7.0.29",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-functional": "^9.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-sonarjs": "^3.0.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.5.3",
    "sass": "^1.89.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.26.1",
    "vite": "^7.0.0",
    "vite-imagetools": "^7.1.0",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "yarn@1.22.22"
}
