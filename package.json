{
  "name": "cgn-onboarding-portal-frontend",
  "version": "3.32.11",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start:uat": "cp .env.uat .env.local && vite --port 3000",
    "start:prod": "cp .env.prod .env.local && vite --port 3000",
    "preview": "vite preview --port 3000",
    "prebuild": "npm-run-all prebuild:*",
    "build": "yarn prebuild && tsc -b && vite build",
    "prebuild:generate": "yarn run generate",
    "prebuild:clean": "rm -rf dist",
    "prebuild:tsc": "tsc -b --noEmit",
    "build:prod": "cp .env.prod .env.local && yarn build",
    "build:uat": "cp .env.uat .env.local && yarn build",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --passWithNoTests --coverage --coverageReporters=cobertura",
    "lint": "eslint .",
    "lint-autofix": "eslint . --fix",
    "generate": "./generate-openapi.sh",
    "prettify": "prettier --write \"./**/*.{ts,tsx}\"",
    "prepare": "husky",
    "tsc:noemit": "tsc --noemit",
    "analyzie-bundle-size": "npx vite-bundle-visualizer -t sunburst"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.39.0",
    "@reduxjs/toolkit": "^1.5.1",
    "@tanstack/react-query": "^4.0.0",
    "axios": "^1.8.4",
    "bootstrap-italia": "^2.14.0",
    "date-fns": "^2.16.1",
    "design-react-kit": "^5.7.1",
    "file-saver": "^2.0.5",
    "formik": "^2.2.6",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-datepicker": "^8.3.0",
    "react-dom": "^19.0.0",
    "react-google-recaptcha": "^2.1.0",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-table": "^7.6.3",
    "reactstrap": "^9.2.3",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "@pagopa/danger-custom-rules": "^2.0.3",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/file-saver": "^2.0.2",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.9",
    "@types/react": "^19.0.10",
    "@types/react-datepicker": "^7.0.0",
    "@types/react-dom": "^19.0.4",
    "@types/react-google-recaptcha": "^2.1.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-table": "^7.0.29",
    "@vitejs/plugin-react": "^4.3.4",
    "danger": "^10.3.0",
    "danger-plugin-digitalcitizenship": "^1.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-functional": "^9.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-sonarjs": "^3.0.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^26.6.3",
    "lint-staged": "^15.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "sass": "1.81.0",
    "ts-jest": "^26.5.4",
    "typescript": "^5.8.3",
