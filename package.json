{
  "name": "cgn-onboarding-portal-frontend",
  "version": "0.5.1",
  "license": "MIT",
  "scripts": {
    "start:prod": "NODE_ENV=production parcel --port 3000 src/index.html --public-url / --out-dir dist/debug",
    "start:uat": "NODE_ENV=uat parcel --port 3000 src/index.html --public-url / --out-dir dist/debug",
    "start:dev": "NODE_ENV=dev parcel --port 3000 src/index.html --public-url / --out-dir dist/debug",
    "prebuild": "npm-run-all prebuild:*",
    "prebuild:generate": "npm-run-all generate:*",
    "prebuild:clean": "shx rm -rf dist",
    "prebuild:tsc": "tsc --noEmit",
    "build": "parcel build src/index.html --out-dir dist --public-url /",
    "build:prod": "NODE_ENV=production yarn run build",
    "build:uat": "NODE_ENV=uat yarn run build",
    "build:dev": "NODE_ENV=dev yarn run build",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --passWithNoTests --coverage --coverageReporters=cobertura",
    "lint": "eslint . -c .eslintrc.js --ext .ts,.tsx --cache",
    "lint-autofix": "eslint . -c .eslintrc.js --ext .ts,.tsx --fix",
    "generate:api": "openapi-generator-cli generate -i ./src/api/api.yaml -g typescript-axios -o ./src/api/generated",
    "generate:api_backoffice": "openapi-generator-cli generate -i ./src/api/api_backoffice.yaml -g typescript-axios -o ./src/api/generated_backoffice",
    "generate:api_public": "openapi-generator-cli generate -i ./src/api/api_public.yaml -g typescript-axios -o ./src/api/generated_public",
    "generate": "npm-run-all generate:*",
    "prettify": "prettier --write \"./**/*.{ts,tsx}\"",
    "typescript:dts": "node scripts/dts.js"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.14.1",
    "@reduxjs/toolkit": "^1.5.1",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.3",
    "bootstrap-italia": "^1.4.3",
    "classnames": "^2.2.6",
    "date-fns": "^2.16.1",
    "design-react-kit": "^3.3.3",
    "file-saver": "^2.0.5",
    "formik": "^2.2.6",
    "fp-ts": "1.17.4",
    "fs": "^0.0.1-security",
    "generated": "^0.0.3",
    "glob": "^7.1.6",
    "i18next": "^19.8.4",
    "io-ts": "1.8.5",
    "jwt-decode": "^3.1.2",
    "node-sass": "^5.0.0",
    "path": "^0.12.7",
    "react": ">=16.4.1",
    "react-datepicker": "^3.8.0",
    "react-dom": "^17.0.1",
    "react-google-recaptcha": "^2.1.0",
    "react-i18next": "^11.7.3",
    "react-redux": "^7.2.3",
    "react-router-dom": "^5.2.0",
    "react-select": "^4.3.1",
    "react-table": "^7.6.3",
    "react-to-typescript-definitions": "^3.0.1",
    "reactstrap": "^8.7.1",
    "src": "^1.1.2",
    "typeface-lora": "^1.1.13",
    "typeface-roboto-mono": "^1.1.13",
    "typeface-titillium-web": "^1.1.13",
    "universal-cookie": "^4.0.4",
    "url": "^0.11.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.2.6",
    "@pagopa/danger-custom-rules": "^2.0.3",
    "@pagopa/eslint-config": "^1.1.1",
    "@pagopa/openapi-codegen-ts": "9.0.0",
    "@pagopa/ts-commons": "9.1.0",
    "@svgr/parcel-plugin-svgr": "^5.5.0",
    "@types/classnames": "^2.2.11",
    "@types/file-saver": "^2.0.2",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.9",
    "@types/react": "^16.9.56",
    "@types/react-datepicker": "^3.1.8",
    "@types/react-dom": "^16.9.9",
    "@types/react-google-recaptcha": "^2.1.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-select": "^4.0.15",
    "@types/react-table": "^7.0.29",
    "cssnano": "^4.1.10",
    "danger": "^10.3.0",
    "danger-plugin-digitalcitizenship": "^1.0.0",
    "dotenv": "^8.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.3",
    "sass": "^1.32.13",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.4",
    "typescript": "^4.1.2"
  },
  "resolutions": {
    "fp-ts": "1.17.4",
    "io-ts": "1.8.5",
    "@babel/preset-env": "7.13.8"
  }
}
