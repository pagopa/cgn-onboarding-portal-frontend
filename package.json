{
  "name": "cgn-onboarding-portal-frontend",
  "version": "3.33.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start:uat": "cp .env.uat-local .env.local && react-router dev",
    "start:prod": "cp .env.prod-local .env.local && react-router dev",
    "preview:uat": "cp .env.uat-local .env.local && yarn build && vite preview",
    "preview:prod": "cp .env.prod-local .env.local && yarn build && vite preview",
    "prebuild": "npm-run-all prebuild:*",
    "build": "yarn prebuild && tsc -b && react-router build && cp -r build/client dist",
    "prebuild:generate": "yarn run generate",
    "prebuild:clean": "rm -rf build && rm -rf dist",
    "prebuild:react-router": "react-router typegen",
    "build:prod": "cp .env.prod .env.local && yarn build",
    "build:uat": "cp .env.uat .env.local && yarn build",
    "build:prod-local": "cp .env.prod-local .env.local && yarn build",
    "build:uat-local": "cp .env.uat-local .env.local && yarn build",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --passWithNoTests --coverage --coverageReporters=cobertura",
    "lint": "eslint .",
    "lint-autofix": "eslint . --fix",
    "generate": "./generate-openapi.sh",
    "prettify": "prettier --write \"./**/*.{ts,tsx}\"",
    "prepare": "husky",
    "postinstall": "patch-package",
    "tsc:noEmit": "tsc -b --noEmit",
    "analyzie-bundle-size": "npx vite-bundle-visualizer -t sunburst"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.39.0",
    "@hookform/lenses": "^0.8.2",
    "@hookform/resolvers": "^5.2.2",
    "@react-router/fs-routes": "^7.9.1",
    "@react-router/node": "^7.9.1",
    "@reduxjs/toolkit": "^1.5.1",
    "@tanstack/react-query": "^5.89.0",
    "@types/lodash": "^4.17.17",
    "axios": "^1.12.2",
    "bootstrap-italia": "^2.16.1",
    "date-fns": "^4.1.0",
    "design-react-kit": "^5.8.3",
    "file-saver": "^2.0.5",
    "isbot": "^5.1.30",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "react": "^19.1.1",
    "react-datepicker": "^8.7.0",
    "react-dom": "^19.1.1",
    "react-google-recaptcha": "^3.1.0",
    "react-hook-form": "^7.62.0",
    "react-redux": "^7.2.3",
    "react-router": "^7.9.1",
    "react-table": "^7.6.3",
    "reactstrap": "^9.2.3",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@jest/globals": "^30.1.2",
    "@openapitools/openapi-generator-cli": "^2.23.4",
    "@react-router/dev": "^7.9.1",
    "@stylistic/eslint-plugin": "^5.3.1",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@svgr/plugin-svgo": "^8.1.0",
    "@types/file-saver": "^2.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-google-recaptcha": "^2.1.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-table": "^7.0.29",
    "@vitejs/plugin-react": "^5.0.2",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-functional": "^9.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-sonarjs": "^3.0.5",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "lint-staged": "^16.1.6",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.5.3",
    "sass": "^1.92.1",
    "ts-jest": "^29.4.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.5",
    "vite-imagetools": "^8.0.0",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "packageManager": "yarn@1.22.22"
}
